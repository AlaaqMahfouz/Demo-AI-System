{
  "dependencies": {
    "@angular/core": "^17.3.3",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "fs-extra": "^11.2.0",
    "groupdocs-parser-cloud": "^23.10.0",
    "next": "^14.2.3",
    "node": "^20.13.1",
    "nvm": "^0.0.4",
    "pdf-image-extractor": "^2.0.8",
    "pdf-img-convert": "^1.2.1",
    "pdf-lib": "^1.17.1",
    "pdf2pic": "^3.1.1",
    "pdfjs-dist": "^4.0.379"
  },
  "scripts": {
    "client:build": "cd client && npm run build",
    "client:start": "cd client && npm run start",
    "pages:build": "cd pages && npm run build",
    "pages:start": "cd pages && npm run start",
    "build": "npm run pages:build && npm run client:build",
    "start": "npm run client:start && npm run pages:start"
  },
  "main": "pages/index.ts",
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": "client/build"
      }
    },
    {
      "src": "pages/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "pages/build"
      }
    }
  ],
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/react": "18.3.2",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": "^20.10.0"
  },
  "directories": {
    "client": "client",
    "pages": "pages"
  }
}
